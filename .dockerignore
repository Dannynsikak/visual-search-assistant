# Ignore node_modules from frontend and backend

frontend/node_modules/
backend/node_modules/

# Ignore Python bytecode files

backend/**/\*.pyc
backend/**/\*.pyo

# Ignore environment files (to prevent leaking sensitive information)

.env
.env.local

# Ignore log files

\*.log

# Ignore Dockerfile and Docker-related files (if you're not using them in the image)

Dockerfile
.dockerignore

# Ignore Vite's development cache and build files

frontend/.vite/
frontend/dist/

# Ignore temporary or build folders

temp/
_.swp
_.swo

# Ignore any IDE or editor configuration files (e.g., VS Code, JetBrains)

.vscode/
.idea/
\*.iml
